{
  "name": "@di-pdfs/ssc-pdf",
  "version": "1.0.0",
  "description": "for creating DI print pdfs",
  "main": "index.js",
  "scripts": {
    "dev": "npm run prebuild && concurrently \"tsc --pretty --watch\" \"next dist\"",
    "prebuild": "mkdir -p dist && cp -R node_modules/@di-pdfs/pdf-base/lib/static/ dist/ && tsc",
    "pull": "npm run build && node dist/pullData/index.js",
    "test": "jest",
    "build": "next build dist",
    "start": "next start dist",
    "lint": "tslint -c tslint.json 'pages/**/*.tsx' 'src/**/*.tsx' 'src/**/*.ts'"
  },
  "keywords": [
    "pdfs"
  ],
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@di-pdfs/api-base": "^1.0.0",
    "@di-pdfs/ssc-api": "^1.0.0",
    "@di-pdfs/pdf-base": "1.0.3",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "next": "latest",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "semantic-ui-react": "^0.77.0"
  },
  "devDependencies": {
    "@types/next": "^2.4.5",
    "@types/node": "^8.0.55",
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.22",
    "@types/storybook__react": "^3.0.5",
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/addon-links": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "less": "^2.7.3",
    "less-plugin-clean-css": "^1.5.1",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.1.0",
    "pretty-format": "^21.2.1",
    "ts-jest": "^21.1.3",
    "typescript": "^2.6.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
