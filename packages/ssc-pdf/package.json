{
  "name": "@devinit/ssc-pdf",
  "version": "1.0.2",
  "description": "for creating DI print pdfs",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "npm run prebuild && concurrently \"tsc --pretty --watch\" \"next dist\"",
    "prebuild": "rm -rf dist && mkdir -p dist && cp -R node_modules/@devinit/pdf-base/lib/static/ dist/static && cp next.config.js dist && tsc",
    "pull": "tsc && node dist/pullData/index.js",
    "test": "jest",
    "build": "next build dist",
    "export": "next export dist",
    "static": "npm run build && npm export",
    "start": "next start dist -p 7777",
    "lint": "tslint -c tslint.json 'pages/**/*.tsx' 'src/**/*.tsx' 'src/**/*.ts'"
  },
  "keywords": [
    "pdfs"
  ],
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@devinit/api-base": "^1.0.7",
    "@devinit/pdf-base": "1.0.3",
    "@devinit/ssc-api": "^1.0.7",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "next": "latest",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/next": "^2.4.7",
    "@types/node": "^8.0.55",
    "@types/react": "16.0.27",
    "concurrently": "^3.5.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.1.0",
    "pretty-format": "^21.2.1",
    "ts-jest": "^21.1.3",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
