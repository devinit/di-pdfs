{
  "name": "@devinit/ssc-api",
  "version": "1.0.6",
  "description": "ssc api",
  "main": "index.js",
  "scripts": {
    "test": "jest src/modules/china/index.test.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start": "npm run build && NODE_ENV=production PORT=8080 node lib/main.js",
    "build": "NODE_ENV=production tsc",
    "build:watch": "NODE_ENV=development tsc -w",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n",
    "gqlToTs": "node node_modules/@devinit/api-base/lib/makeTypeDefs",
    "dev": "NODE_ENV=development concurrently \"npm run build:watch\" \"node-dev lib/main.js\""
  },
  "keywords": [
    "api",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/di-pdfs.git"
  },
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@devinit/api-base": "^1.0.6",
    "@devinit/charts": "^1.6.29",
    "@devinit/pdf-base": "1.0.4",
    "ramda": "^0.25.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "7.0.28",
    "@types/ramda": "^0.24.7",
    "@types/shortid": "0.0.29",
    "concurrently": "^3.5.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.1.0",
    "node-dev": "^3.1.3",
    "pretty-format": "^21.2.1",
    "ts-jest": "^21.1.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
