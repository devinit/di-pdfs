{
  "name": "@devinit/print",
  "version": "1.0.0",
  "description": "pdf print utilities",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "test": "npm run canary && jest",
    "canary": "canary --remote-debugging-port=9222 --disable-gpu --headless",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "keywords": [
    "pdfs", "chromeless", "poppler"
  ],
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "dependencies": {
    "chromeless": "latest",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/next": "^2.4.7",
    "@types/node": "^8.0.55",
    "jest": "^21.2.1",
    "jest-cli": "^21.1.0",
    "pretty-format": "^21.2.1",
    "ts-jest": "^21.1.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
