{
  "name": "@devinit/print",
  "version": "1.0.2",
  "description": "pdf print utilities",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "ts-node src/ssc/index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "keywords": [
    "pdfs",
    "chromeless",
    "poppler"
  ],
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "dependencies": {
    "puppeteer": "1.1.1",
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "@types/node": "9.4.6",
    "ts-jest": "22.0.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
