{
  "extends": ["tslint:recommended", "tslint-react"],
    "rules": {
      "no-console": [false],
      "quotemark": [true, "single", "jsx-double"],
      "trailing-comma": [false],
      "no-var-requires": false,
      "curly": false,
      "jsx-no-multiline-js": false,
      "object-literal-sort-keys": false,
      "arrow-parens":false,
      "variable-name":false,
      "ordered-imports": [false]
    }
  }
  